{
  "name": "linter-ui-touchbar",
  "main": "./lib/index.js",
  "version": "0.1.1",
  "description": "linter-ui package for Apple Touch Bar",
  "keywords": [],
  "repository": "https://github.com/haklop/linter-ui-touchbar.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^3.2.2",
    "eslint-config-xo-space": "^0.15.0"
  },
  "providedServices": {
    "linter-ui": {
      "versions": {
        "1.0.0": "provideUI"
      }
    }
  },
  "consumedServices": {
